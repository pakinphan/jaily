{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/input.css",
        "use": "@sveltejs/adapter-static",
        "config": {
          "output": "/assets"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.js"
      }
    ],
    "build": {
      "commands": {
        "build": "npm run build"
      },
      "use": "@vercel/node"
    }
  }
  